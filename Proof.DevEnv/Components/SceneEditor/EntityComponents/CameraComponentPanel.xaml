<UserControl x:Class="Proof.DevEnv.Components.EntityComponents.CameraComponentPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Proof.DevEnv.Components.EntityComponents"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Expander Header="Camera Component" IsExpanded="True">
        <StackPanel x:Name="Body">
            <Button x:Name="Active" Content="Active" Click="Active_Click" />
            <Button x:Name="Remove" Content="Remove" Click="Remove_Click" />
        </StackPanel>
    </Expander>
</UserControl>
